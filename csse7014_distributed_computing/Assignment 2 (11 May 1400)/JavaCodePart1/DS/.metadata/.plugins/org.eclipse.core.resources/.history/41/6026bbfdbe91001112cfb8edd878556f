module exchange {

	interface TemperatureHomemanager {
		void temperatureData(int data);
	};
	
	interface BpheartrateHomemanager {
		void bpHeartrateData(int bpData, int heartrateData, string user);
	};
	
	interface UIHomemanager {
		int getTemperature();
		string getBpHeartrate();
	};
};